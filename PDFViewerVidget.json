{
	"name": "PDF Viewer Vidget",
	"description": "Displays a PDF file in popup-window",
	"eula": "",
	"vendor": "Promos",
	"license": "",
	"id": "com.openpromos.sac.pdf.vidget",
	"newInstancePrefix": "PDFViewer",
	"version": "0.0.1",
	"icon": "data:image/png;base64,",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-openpromos-sac-pdf-vidget",
			"url": "https://raw.githubusercontent.com/IwanSchadrin/sac_pdf_vidget_no_pages/main/WebComponent.js",
			"integrity": "",
			"ignoreIntegrity": true
		},
		{
			"kind": "styling",
			"tag": "com-openpromos-sac-pdf-vidget-aps",
			"url": "https://raw.githubusercontent.com/IwanSchadrin/sac_pdf_vidget_no_pages/main/aps_WebComponent.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"pdfUrl": {
			"type": "string",
			"description": "PDF URL",
			"default": ""
		},
		"popupTitle": {
			"type": "string",
			"description": "Title",
			"default": "Steckbrief"
		},		
		"displayLink": {
			"type": "boolean",
			"description": "Display Link",
			"default": true
		}
	},
	"methods": {
		"getDisplayLink": {
			"description": "Returns true if the link for popup should be visible",
			"body": "return this.displayLink;",
			"returnType": "boolean"
		},
		"setDisplayLink": {
			"description": "Defines if the link for popup should be visible. If invisible, use the displayPDFInPopup method in custom script.",
			"parameters": [
				{
					"name": "displayLink",
					"type": "boolean",
					"description": "True if the link should be visible"
				}
			],
			"body": "this.displayLink = displayLink;"
		},
		"getPdfUrl": {
			"description": "Returns the URL of the PDF file",
			"body": "return this.pdfUrl;",
			"returnType": "string"
		},
		"setPdfUrl": {
			"description": "Sets the URL of the PDF file",
			"parameters": [
				{
					"name": "newPdfUrl",
					"type": "string",
					"description": "The new PDF URL"
				}
			],
			"body": "this.pdfUrl = newPdfUrl;"
		},
		"setPopupTitle": {
			"description": "Sets the title of the popup-window in that the PDF file will be displayed",
			"parameters": [
				{
					"name": "newPopupTitle",
					"type": "string",
					"description": "The new title"
				}
			],
			"body": "this.popupTitle = newPopupTitle;"
		},		
		"getPopupTitle": {
			"description": "Returns the title of the popup-window in that the PDF file will be displayed",			
			"body": "return this.popupTitle;",
			"returnType": "string"
		},

		"displayPDFInPopup": {
			"description": "Displays the pop-up window"
		}
	},
	"events": {
	}
}
